{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb",
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "indent": [
            true
        ],
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        // Pending fix for shorthand property names.
        "object-literal-sort-keys": false,
        "import-name": false,
        "trailing-comma": false,
        "no-console": {
            "severity": "warning"
        },
        "member-access": false
    },
    "rulesDirectory": []
}